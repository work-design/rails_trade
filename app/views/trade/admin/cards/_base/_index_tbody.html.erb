<td>
  <p><%= model.card_template.name %></p>
  <p class="has-text-grey"><%= model.card_uuid %></p>
</td>
<td>
  <p><%= model.user&.name %></p>
</td>
<td>
  <p><time data-controller="time"><%= model.effect_at.to_fs(:iso8601) %></time></p>
  <p>
  <% if model.expire_at && model.expire_at > Date.today %>
    <time data-controller="time" class="has-text-success"><%= model.expire_at.to_fs(:iso8601) %></time>
  <% elsif model.expire_at %>
    <time data-controller="time" class="has-text-danger"><%= model.expire_at.to_fs(:iso8601) %></time>
  <% else %>
    <span class="has-text-success">长期</span>
  <% end %>
  </p>
</td>
<td><%= model.temporary_i18n %></td>
<td><time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time></td>
