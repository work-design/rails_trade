<% if referer_path == "trade/#{params[:namespace]}/orders/_index" %>
  <%= turbo_stream.replace "tr_#{@order.id}", partial: 'trade/admin/orders/_base/index_tbody', layout: 'trade/admin/orders/_base/index_tr', locals: { model: @order } %>
  <%= turbo_stream.remove 'modal' %>
<% else %>
  <%= turbo_stream.append 'body', partial: 'visit' %>
<% end %>
