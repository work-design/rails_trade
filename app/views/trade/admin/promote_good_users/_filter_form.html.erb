<%= form_with theme: 'search', model: Trade::PromoteGood.new do |f| %>
  <div class="field-body">
    <%= f.text_field :good_type %>
    <%= f.text_field :good_id %>
    <%= f.text_field :member_id %>
    <%= f.text_field :member_organ_id %>
    <%= f.text_field :user_id %>
    <div class="field is-narrow">
      <%= f.submit %>
    </div>
  </div>
<% end %>

