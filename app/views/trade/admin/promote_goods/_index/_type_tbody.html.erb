<tr>
  <td><%= model[0] %></td>
  <td>
    <div class="buttons are-small">
      <%= button_to({ action: 'new', good_type: model[1] }, class: 'button is-small is-link') do %>
        <i class="fa-solid fa-plus"></i>
        <span class="ml-1"><%= t('.new.title') %></span>
      <% end %>
      <%= button_to({ action: 'part_new', good_type: model[1] }, class: 'button is-small is-link') do %>
        <i class="fa-solid fa-plus"></i>
        <span class="ml-1"><%= t('.part_new.title') %></span>
      <% end %>
    </div>
  </td>
  <td>
    <%= link_to({ action: 'part', good_type: model[1] }, text: true) do %>
      <span><%= Trade::PromoteGood.available.where(good_type: model[1]).where.not(good_id: nil).count %></span>
    <% end %>
  </td>
</tr>